  <script src="../vendor/docs.min.js"></script>
  <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
  <script src="../vendor/ie10-viewport-bug-workaround.js"></script>
  <script src="../js/app.js"></script>
</body>

</html>
